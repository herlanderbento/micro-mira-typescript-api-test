                    "type": "string",
                    "example": "male"
                  },
                  "profession": {
                    "required": true,
                    "type": "string",
                    "example": "Software Developer"
                  },
                  "yearExperience": {
                    "required": true,
                    "type": "number",
                    "example": 4
                  },
                  "educationLevel": {
                    "required": true,
                    "type": "string",
                    "example": "university"
                  },
                  "street": {
                    "type": "string",
                    "example": "123 Main St"
                  },
                  "city": {
                    "type": "string",
                    "example": "New York"
                  },
                  "country": {
                    "type": "string",
                    "example": "Angola"
                  },
                  "biography": {
                    "type": "string",
                    "example": "some biography"
                  },
                  "birthdate": {
                    "type": "date",
                    "example": "2024-01-15T13:23:16.655+00:00"
                  },
                  "isWork": {
                    "type": "boolean",
                    "example": false
                  },
                  "isFreelancer": {
                    "type": "boolean",
                    "example": false
                  }
                }
              }
